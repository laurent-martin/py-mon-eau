  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test tout sur mon eau",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/toutsurmoneau/__main__.py",
      "cwd": "${workspaceFolder}",
      "args": [
        "--config",
        "${workspaceFolder}/private/vars.env",
        "-e",
        "meter_id",
        "--debug"
      ],
      "stopOnEntry": false,
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${PYTHONPATH}:${workspaceFolder}"
      }
    }
  ]
}
