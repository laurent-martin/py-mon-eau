  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test tout sur mon eau",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/toutsurmoneau/__main__.py",
      "cwd": "${workspaceFolder}",
      "args": ["--debug", "-U", "${env:R}", "-u", "${env:U}", "-p", "${env:P}", "-e", "meter_id"],
      "stopOnEntry": false,
      "justMyCode": false,
      "envFile": "${workspaceFolder}/private/env.sh",
      "env": {
        "PYTHONPATH": "${PYTHONPATH}:${workspaceFolder}"
      }
    }
  ]
}
